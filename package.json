{
  "name": "@qiudeng/tencent-cloud-sdk",
  "version": "0.0.1",
  "description": "Tencent Cloud API SDK for CVM instances and TAT commands",
  "type": "module",
  "main": "./dist/tencent-cloud-sdk.cjs",
  "module": "./dist/tencent-cloud-sdk.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/tencent-cloud-sdk.es.js",
      "require": "./dist/tencent-cloud-sdk.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "tencent-cloud",
    "cvm",
    "tat",
    "sdk",
    "api"
  ],
  "author": "qiudeng",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/qiudeng/tencent-cloud-sdk.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.7.0"
  }
}
