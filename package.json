{
  "name": "@qiudeng/tencent-cloud-sdk",
  "version": "0.0.5",
  "description": "Tencent Cloud API SDK for k8s usage",
  "type": "module",
  "main": "./dist/tencent-cloud-sdk.cjs.js",
  "module": "./dist/tencent-cloud-sdk.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/tencent-cloud-sdk.es.js",
      "require": "./dist/tencent-cloud-sdk.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "browser": "unbuildable",
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "prepublishOnly": "pnpm run build",
    "docs": "typedoc",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "keywords": [
    "tencent-cloud",
    "cvm",
    "tat",
    "sdk",
    "api"
  ],
  "author": "qiudeng",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/qiudeng/tencent-cloud-sdk.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^24.10.4",
    "dotenv": "^17.2.3",
    "typedoc": "^0.28.15",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^4.0.16"
  }
}
